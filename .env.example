# Copy to .env and set your OpenAI API key
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
PORT=3000
# Optional: set a proxy token to require requests include X-Proxy-Token header
PROXY_TOKEN=
# Rate limit window in minutes and max requests per IP
RATE_LIMIT_WINDOW_MIN=60
RATE_LIMIT_MAX=200
# Optional model override (default gpt-3.5-turbo)
OPENAI_MODEL=gpt-3.5-turbo
