{
  "name": "crop-health-analyzer-proxy",
  "version": "1.0.0",
  "description": "Small proxy to call OpenAI for Crop Health Analyzer demo",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "test:integration": "jest --runInBand --testMatch '**/*(*.integration).test.js'",
    "lint": "eslint .",
    "build": "mkdir -p dist && cp -r *.html *.js *.css dist/",
    "ci": "npm run lint && npm test"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  }
}
